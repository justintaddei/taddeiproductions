{
  "name": "taddeiproductions",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:portfolio_manager": "tsc --esModuleInterop portfolio_manager/index.ts --outDir ./portfolio_manager/dist/ --watch",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "portfolio:generate": "tsc --esModuleInterop ./portfolio_manager/index.ts --outDir ./portfolio_manager/dist/ && node portfolio_manager/dist/index",
    "portfolio:save": "npm run portfolio:generate && git add . && git commit -m \"Updating portfolio\" && git push"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "^5.0.0-1602966760.822bc05",
    "@nuxtjs/axios": "^5.12.2",
    "illusory": "^2.0.3",
    "nuxt": "^2.14.7",
    "v-shared-element": "^3.0.3",
    "v-wave": "^1.2.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/svg": "^0.1.12",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "sass-loader": "^9.0.3",
    "svg-inline-loader": "^0.8.2",
    "typescript": "^4.0.5"
  }
}
